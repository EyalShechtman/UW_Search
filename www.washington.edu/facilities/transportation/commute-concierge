<section id="block-views-sidebar-block-page-sidebar-maps" class="block block-views widget   clearfix">

      
  <div class="view view-sidebar view-id-sidebar view-display-id-block_page_sidebar_maps sidebar-card-inner dark-gold view-dom-id-45b174ee5f509ac3ab30a407e2735f82">
            <div class="view-header">
      <h2 class="widgettitle uwf-ic-inline-text">Maps <span class="uwf-ic-map-marker-thin-ol uwf-xs white"> </span></h2>    </div>
  
  
  
      <div class="view-content">
      <div class="item-list">    <ul>          <li class="views-row views-row-1 views-row-odd views-row-first">  
  <div class="views-field views-field-title-1">        <span class="field-content"><a href="https://transportation.uw.edu/maps/bike-parking">Bike lockers, racks &amp; rooms</a></span>  </div></li>
          <li class="views-row views-row-2 views-row-even">  
  <div class="views-field views-field-title-1">        <span class="field-content"><a href="https://transportation.uw.edu/maps/bus-stops-light-rail">Bus stops &amp; light rail</a></span>  </div></li>
          <li class="views-row views-row-3 views-row-odd views-row-last">  
  <div class="views-field views-field-title-1">        <span class="field-content"><a href="https://transportation.uw.edu/maps/ride-hailing-pick-up">Ride hailing pickup</a></span>  </div></li>
      </ul></div>    </div>
  
  
  
  
  
  
</div>
</section>
  </div>
