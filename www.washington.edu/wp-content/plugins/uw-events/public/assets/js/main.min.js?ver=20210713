"use strict";jQuery(function(t){var s=t("#uw-events.filter");s.isotope({itemSelector:".single-event",percentPosition:!0,masonry:{columnWidth:".single-event",gutter:20}});var i=t("#uw-event-filter");i.find("button").click(function(){var e=t(this);if(e.hasClass("selected"))return!1;i.find(".selected").removeClass("selected"),e.addClass("selected");e=t(this).attr("data-filter");s.isotope({filter:e})}),s.imagesLoaded().progress(function(){s.isotope("layout")})});